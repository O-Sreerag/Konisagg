        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Users list</h2>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Show all</option>
                                <option>Active</option>
                                <option>Blocked</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>User</th>
                                    <th>Joined at</th>
                                    <th>Email</th>
                                    <th>Status</th>
                                    <th> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each userData}}
                                <tr>
                                    <td width="40%">
                                            {{#if userimage}}
                                                <div class="left">
                                                    <img src="../multer/users/{{userimage}}" class="img-sm img-avatar" alt="Userpic">
                                                </div>
                                            {{else}}
                                                <div class="left">
                                                    <img src="../images/user-icon-2.png" class="img-sm img-avatar" alt="Default Userpic">
                                                </div>
                                            {{/if}}
                                            <div class="info pl-3">
                                                <h6 class="mb-0 title">{{username}}</h6>
                                                <small class="text-muted">User ID: {{_id}}</small>
                                            </div>
                                    </td>
                                    <td width="15%">
                                        <script>
                                            var createdAt = "{{createdAt}}"; 
                                            var slicedValue = createdAt.slice(0, 10);
                                            document.write(slicedValue); 
                                        </script>
                                    </td>
                                    <td  width="15%">{{useremail}}</td>
                                    {{#if status}}
                                    <td  width="15%"><span class="badge rounded-pill alert-success">Active</span></td>
                                    {{else}}<td  width="15%"><span class="badge rounded-pill alert-danger">Blocked</span></td>
                                    {{/if}}
                                    <td  width="15%">
                                        {{#if status}}<a href="../admin/userBlock?userid={{_id}}" class="btn btn-sm btn-danger rounded font-sm mt-15">Block</a>
                                        {{else}}<a href="../admin/userUnblock?userid={{_id}}" class="btn btn-sm btn-success rounded font-sm mt-15" style="color: white !important;">Unblock</a>
                                        {{/if}}
                                    </td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table> <!-- table-responsive.// -->
                    </div>
